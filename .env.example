# DO NOT ADD SECRETS TO THIS FILE. This is a good place for defaults.
# If you want to add secrets use `.env.production.local` instead, which is automatically detected by `docker compose`.

# DEVELOPMENT TOOLS
# Ideally, don't add them to production deployment envs
# !STARTERCONF Change to true if you want to log data
NEXT_PUBLIC_APP_SUPPORT_VERSION='v1.0'
NEXT_PUBLIC_SHOW_LOGGER="false"
NEXT_PUBLIC_DEBUG=true

# TODO for https and production
NEXT_SERVER_API_URL="http://web-app:8000/api"
NEXT_PUBLIC_API_URL='https://api.newapp.local/api'

# TODO for https and development
# NEXT_SERVER_API_URL="http://api.newapp.local/api"
# NEXT_PUBLIC_API_URL='http://api.newapp.local/api'

NEXT_PUBLIC_API_MOCKING=false
# NEXT_PUBLIC_API_MOCKING=true

ENV_VARIABLE=production_server_only_variable
NEXT_PUBLIC_ENV_VARIABLE=production_public_variable
# Path: .env

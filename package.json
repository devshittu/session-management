{
  "name": "actman",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "prebuild": "yarn prisma:generate-schema",
    "postinstall": "yarn prisma:generate-schema && yarn prisma generate",
    "build": "next build",
    "start": "next start",
    "prettier": "prettier \"**/*.+(json|ts|tsx)\"",
    "format:fix": "npm run prettier -- --write",
    "format:check": "npm run prettier -- --check",
    "types:check": "tsc --noEmit --pretty",
    "lint": "eslint --ext .ts,.tsx ./src ",
    "lint:fix": "eslint --ext .ts,.tsx ./src --fix",
    "prepare": "husky",
    "prisma:format": "dotenv -e .env -- npx prisma format",
    "prisma:migrate": "dotenv -e .env -- npx prisma migrate dev",
    "prisma:push": "dotenv -e .env -- npx prisma db push",
    "prisma:studio": "dotenv -e .env -- npx prisma studio",
    "prisma:generate-schema": "ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/generate-schema.ts",
    "prisma:generate-:schema:prod": "dotenv -e .env.prod ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/generate-schema.ts",
    "prisma:migrate:dev": "dotenv -e .env -- npx prisma migrate dev --schema=prisma/schema.prisma",
    "prisma:generate": "npx prisma generate --schema=prisma/schema.prisma",
    "prisma:seed:dev": "dotenv -e .env -- npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "prisma:seed:prod": "npx ts-node --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "db:reset:dev": "dotenv -e .env -- npx prisma migrate reset --force",
    "db:reset:prod": "npx prisma migrate reset --force",
    "prisma:generate-client": "yarn prisma generate"
  },
  "dependencies": {
    "@floating-ui/react": "^0.27.1",
    "@prisma/client": "^5.22.0",
    "@tanstack/react-query": "^5.60.6",
    "@tanstack/react-query-devtools": "^5.60.6",
    "axios": "^1.7.7",
    "date-fns": "^4.1.0",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.14.4",
    "next": "15.0.3",
    "next-themes": "^0.4.3",
    "pg": "^8.13.1",
    "prismjs": "^1.29.0",
    "react": "19.0.0-rc-66855b96-20241106",
    "react-dom": "19.0.0-rc-66855b96-20241106",
    "react-focus-lock": "^2.13.5",
    "react-icons": "^5.3.0",
    "react-intersection-observer": "^9.13.1",
    "react-toastify": "^10.0.6",
    "react-transition-group": "^4.4.5",
    "recharts": "^2.13.3",
    "tailwind-merge": "^2.5.5",
    "zustand": "^5.0.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.2.0",
    "@tanstack/eslint-plugin-query": "^5.60.1",
    "@types/date-fns": "^2.6.3",
    "@types/node": "^22.9.0",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@types/react-transition-group": "^4.4.12",
    "daisyui": "^4.12.14",
    "eslint": "^8",
    "eslint-config-next": "15.0.3",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "husky": "^9.1.6",
    "lint-staged": "^15.2.10",
    "postcss": "^8",
    "prettier": "^3.3.3",
    "prisma": "^5.22.0",
    "tailwindcss": "^3.4.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
